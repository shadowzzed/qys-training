<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qys.training.biz.hazelcast.mapper.SysConfigMapper">
    <select id="getConfigValue" parameterType="java.lang.String" resultType="java.lang.String">
        select config_value from sys_config where config_key = #{configKey}
    </select>
</mapper>